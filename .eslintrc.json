{
  "env": {
    "browser": true,
    "jest": true
  },
  "extends": [
    "airbnb",
    "plugin:flowtype/recommended",
    "prettier",
    "prettier/flowtype",
    "prettier/react"
  ],
  "plugins": [
    "flowtype",
    "compat",
    "prettier"
  ],
  "rules": {
    "no-unexpected-multiline": 2,
    "compat/compat": 2
  },
  "settings": {
    "polyfills": ["fetch"]
  }
}
